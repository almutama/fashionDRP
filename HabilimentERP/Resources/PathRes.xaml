<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--部件右上角最小化、最大化、关闭按钮用到的路径数据-->
    <Geometry x:Key="MinGeometry">M 0 0 h 10</Geometry>
    <Geometry x:Key="MaxGeometry">M 0 0 h 10 v 10 h -10 z</Geometry>
    <Geometry x:Key="CloseGeometry">M 0 0 l 8 8 M 8 0 L 0 8</Geometry>

    <!--向左向右-->
    <Geometry x:Key="ArrowLeft">M0,5 L10,10 10,0Z</Geometry>
    <Geometry x:Key="ArrowRight">M0,0 L10,5 L0,10Z</Geometry>

    <!--右下角调整部件大小的路径-->
    <EllipseGeometry x:Key="GripGeometry" Center="3,3" RadiusX="3" RadiusY="3" />
    
    <Style x:Key="GripPath" TargetType="Path">
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
        <Setter Property="Margin" Value="0 0 3 3" />
        <Setter Property="Cursor" Value="SizeNWSE" />
        <Setter Property="Data" Value="{StaticResource GripGeometry}" />
        <Setter Property="StrokeThickness" Value="0.5" />
        <Setter Property="Stroke" Value="#000" />
        <Setter Property="Fill" Value="#888" />
    </Style>

    <!--按钮路径样式-->
    <Style x:Key="BasicButtonPath" TargetType="Path">
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="Stroke" Value="{StaticResource LineButtonBrush}" />
        <Setter Property="Fill" Value="{StaticResource LineButtonBrush}" />
    </Style>

